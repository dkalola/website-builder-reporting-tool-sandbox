  <script src="assets/vendor/swiper/swiper-bundle.min.js"></script>
  <script src="assets/vendor/purecounter/purecounter_vanilla.js"></script>

  <!-- Template Main JS File -->
  <script src="assets/js/main.js"></script>


  <script>
     $(document).ready(function () {
    // Select the form and add a submit event listener
    $("#quote").submit(function (event) {
      // Prevent the default form submission behavior
      event.preventDefault();

      // Show the loading message
      $(".loading").show();

      // Serialize the form data to send it to the server
      var formData = $(this).serialize();

      // Send an AJAX request to the server to submit the form data
      $.ajax({
        type: "POST", // Change this to your desired HTTP method (POST, GET, etc.)
        url: "/submit-quote", // Replace with the URL to which you want to submit the form data
        data: formData,
        success: function (response) {
          // Hide the loading message
          $(".loading").hide();

          if (response.success) {
            // Show the success message received from the server
            $(".sent-message").show();
            $(".error-message").hide(); // Hide any previous error message

            // Reset the form after submission
            $("#quote")[0].reset();
          } else {
            // Show the error message received from the server
            $(".error-message").text("An error occurred. Please try again later.").show();
            $(".sent-message").hide(); // Hide the success message (if displayed)
          }
        },
        error: function (error) {
          // Hide the loading message
          $(".loading").hide();

          // Show the error message for Ajax error (e.g., server not responding)
          $(".error-message").text("An error occurred. Please try again later.").show();
          $(".sent-message").hide(); // Hide the success message (if displayed)
        },
      });
    });
  });
  </script>



 
</body>

</html>