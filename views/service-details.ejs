  ======================================================== -->
</head>

<body>

  <!-- ======= Header ======= -->
  <%- include('nav') %>

  <main id="main">

    <!-- ======= Breadcrumbs ======= -->
    <div class="breadcrumbs d-flex align-items-center" style="background-image: url('assets/img/breadcrumbs-bg.jpg');">
      <div class="container position-relative d-flex flex-column align-items-center" data-aos="fade">

        <h2>Service Details</h2>
        <ol>
          <li><a href="/">Home</a></li>
          <li>Service Details</li>
        </ol>

      </div>
    </div><!-- End Breadcrumbs -->

    <!-- ======= Service Details Section ======= -->
    <section id="service-details" class="service-details">
      <div class="container" data-aos="fade-up" data-aos-delay="100">

        <div class="row gy-4">

          <div class="col-lg-4">
            <div class="services-list">
              <a href="/service-d?service=PE" class="<%= data.id === 'PE' ? 'active' : '' %>">Precision Engineering</a>
              <a href="/service-d?service=PS" class="<%= data.id === 'PS' ? 'active' : '' %>">Prototyping Solutions</a>
              <a href="/service-d?service=CCS" class="<%= data.id === 'CCS' ? 'active' : '' %>">Custom Casting Solutions</a>
              <a href="/service-d?service=MA" class="<%= data.id === 'MA' ? 'active' : '' %>">Material Analysis</a>
              <a href="/service-d?service=MS" class="<%= data.id === 'MS' ? 'active' : '' %>">Machining Services</a>
              <a href="/service-d?service=QI" class="<%= data.id === 'QI' ? 'active' : '' %>">Quality Inspection</a>
            </div>
         </div>

          <div class="col-lg-8">
            <img src="<%= data.img %>" alt="" class="img-fluid services-img">
            <h3><%= data.title %></h3>
            <% data.body.forEach(para => { %>
                <p><%= para %></p>
            <% }) %>
          </div>

        </div>

      </div>
    </section><!-- End Service Details Section -->

  </main><!-- End #main -->

  <!-- ======= Footer ======= -->
  <%- include('footer') %>
  <!-- End Footer -->

  <a href="#" class="scroll-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>

  <div id="preloader"></div>

  <!-- Vendor JS Files -->
  <script src="assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
  <script src="assets/vendor/aos/aos.js"></script>
  <script src="assets/vendor/glightbox/js/glightbox.min.js"></script>
  <script src="assets/vendor/isotope-layout/isotope.pkgd.min.js"></script>
  <script src="assets/vendor/swiper/swiper-bundle.min.js"></script>
  <script src="assets/vendor/purecounter/purecounter_vanilla.js"></script>


  <!-- Template Main JS File -->
  <script src="assets/js/main.js"></script>

</body>

</html>